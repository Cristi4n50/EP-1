<%- model_class = Datoscontacto -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:apellido) %></th>
      <th><%= model_class.human_attribute_name(:direccion) %></th>
      <th><%= model_class.human_attribute_name(:email) %></th>
      <th><%= model_class.human_attribute_name(:movil) %></th>
      <th><%= model_class.human_attribute_name(:nombre) %></th>
      <th><%= model_class.human_attribute_name(:telefono) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @datoscontactos.each do |datoscontacto| %>
      <tr>
        <td><%= link_to datoscontacto.id, datoscontacto_path(datoscontacto) %></td>
        <td><%= datoscontacto.apellido %></td>
        <td><%= datoscontacto.direccion %></td>
        <td><%= datoscontacto.email %></td>
        <td><%= datoscontacto.movil %></td>
        <td><%= datoscontacto.nombre %></td>
        <td><%= datoscontacto.telefono %></td>
        <td><%=l datoscontacto.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_datoscontacto_path(datoscontacto), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      datoscontacto_path(datoscontacto),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_datoscontacto_path,
            :class => 'btn btn-primary' %>
