<%= simple_form_for @nota, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :items %>
  <%= f.input :num_documento_recibe %>
  <%= f.input :unidad %>
  <%= f.input :tipo %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                notas_path, :class => 'btn' %>
  </div>
<% end %>
