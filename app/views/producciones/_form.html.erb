<%= simple_form_for @produccion, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :fecha_envio %>
  <%= f.input :fecha_llegada %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                producciones_path, :class => 'btn' %>
  </div>
<% end %>
